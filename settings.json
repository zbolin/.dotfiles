{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [

        {
            "command": "toggleAlwaysOnTop",
            "keys": "ctrl+p"
        },
        {
            "command": "toggleFocusMode",
            "keys": "ctrl+f"
        },
        {
            "command": "unbound",
            "keys": "alt+f"
        },
        {
            "command":
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command":
            {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command":
            {
                "action": "closeTab"
            },
            "keys": "ctrl+w"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        }
    ],
    "alwaysShowTabs": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
