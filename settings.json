{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command": "toggleAlwaysOnTop",
            "keys": "ctrl+p"
        },
        {
            "command":
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "toggleFocusMode",
            "keys": "ctrl+f"
        },
        {
            "command": "unbound",
            "keys": "alt+f"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command":
            {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        }
    ],
    "alwaysShowTabs": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
    "disabledProfileSources":
    [
        "Windows.Terminal.Azure"
    ],
    //"initialCols": 170,
    //"initialPosition": "100,100",
    //"initialRows": 40,
    "language": "en-US",
    "profiles":
    {
        "defaults":
        {
            "bellStyle": "none",
            "colorScheme": "DraculaPlus",
            "cursorShape": "filledBox",
            "padding": "5, 5, 5, 5",
            "startingDirectory": "%USERPROFILE%/GitProjects/",
            "suppressApplicationTitle": false
        },
        "list":
        [
            {
                "commandline": "cmd.exe /k %USERPROFILE%/GitProjects/cmd/aliases.cmd",
                "experimental.retroTerminalEffect": true,
                "font":
                {
                    "face": "Classic Console Neue",
                    "size": 32,
                    "weight": "normal"
                },
                "guid": "{adf151af-2615-4e3a-b497-84e682e8af2e}",
                "hidden": false,
                "name": "Command Prompt"
            },
            {
                "font":
                {
                    "face": "Source Code Pro for Powerline",
                    "size": 12,
                    "weight": "normal"
                },
                "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
                "hidden": false,
                "name": "Windows PowerShell",
                //"opacity": 99,
                "useAcrylic": false
            },
            {
                "experimental.pixelShaderPath": "%USERPROFILE%/GitProjects/windows-terminal-shaders/crt.hlsl",
                "font":
                {
                    "face": "Classic Console Neue",
                    "size": 32,
                    "weight": "normal"
                },
                "guid": "{6a7b96f4-fd08-476e-a57c-32b56b9a4ebc}",
                "hidden": false,
                "name": "Windows PowerShell",
                "scrollbarState": "hidden"
            }
        ]
    },
